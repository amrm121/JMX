                <stringProp name="filename"></stringProp>
                <stringProp name="script">String line = vars.get(&quot;ids_limpo_rec&quot;);

String paramRecs = &quot;&quot;;

String[] words = line.split(&quot;,&quot;);
for (int i = 0; i &lt; words.length; i++) {
  // log.info(words[i]);
  //if (i == 2) {
  //    log.info(&quot;Third word is: &quot; + words[i]);
      vars.put(&quot;id_rec_&quot; + i, words[i]);

      if(paramRecs != &quot;&quot;)
	      paramRecs += &quot;&amp;ids=&quot; +  words[i];
	 else
	      paramRecs = &quot;ids=&quot; +  words[i];
}

vars.put(&quot;paramRecs2&quot;, paramRecs);

//if(line == &quot;&quot;)
//	      vars.put(&quot;ids_limpo&quot;, 0);


	</stringProp>
